{
  "name": "mars-h5",
  "version": "0.0.1-3",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:test": "tsc && vite build --mode test",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run jest && npm run build",
    "release": "standard-version",
    "postrelease": "git push && git push --tags --no-verify",
    "version-tag:major": "npm run release -- --release-as major",
    "version-tag:minor": "npm run release -- --release-as minor",
    "version-tag:patch": "npm run release -- --release-as patch",
    "version-tag:premajor": "npm run version-tag:major -- --prerelease",
    "version-tag:preminor": "npm run version-tag:minor -- --prerelease",
    "version-tag:prepatch": "npm run version-tag:patch -- --prerelease",
    "version-tag:prerelease": "npm run release -- --prerelease"
  },
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.6",
    "@mantine/core": "^6.0.0",
    "@mantine/form": "^6.0.8",
    "@mantine/hooks": "^6.0.0",
    "axios": "^1.3.6",
    "bignumber.js": "^9.1.1",
    "ethers": "^5",
    "foca": "^1.3.1",
    "i18next": "^22.4.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "wagmi": "^0.12.10"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-svgr": "^2.4.0"
  }
}
